<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.raisc.dnaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DNAApp">
        <activity
            android:name=".SplashScreen"
            android:theme="@style/Theme.DNAApp.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.theartofdev.edmodo.cropper.CropImageActivity"
            android:theme="@style/Base.Theme.AppCompat" />
        <activity
            android:name=".newproject.NewProject"
            android:label="New Projects"
            android:theme="@style/Theme.DNAApp.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:label="Projects" />
        <activity
            android:name=".incompleteproject.IncompleteActivity"
            android:theme="@style/Theme.DNAApp.NoActionBar" />
        <activity
            android:name=".completeproject.CompletedActivity"
            android:theme="@style/Theme.DNAApp.NoActionBar" />
        <activity
            android:name=".pendingproject.PendingActivity"
            android:theme="@style/Theme.DNAApp.NoActionBar" />
    </application>

</manifest>